@model List<UserAdd.Models.User>


@if (Model.Count == 0)
{
    <h3 class="text-error">No items matched your search query!</h3>
}
else
{
   foreach (var user in Model)
    {
        <div class="result-section">
            <h3>First Name :</h3>
            <p>@user.FirstName</p>
            <h3>Last Name :</h3>
           <p>@user.LastName</p> 
        </div>

    }
  

}
